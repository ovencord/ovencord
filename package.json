{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@discordjs/discord.js",
	"version": "0.0.0",
	"description": "A powerful library for interacting with the Discord API",
	"private": true,
	"scripts": {
		"build": "bun --filter '*' run build",
		"test": "bun --filter '*' run test",
		"lint": "bun --filter '*' run lint",
		"format": "bun --filter '*' run format",
		"update": "bun --recursive update --interactive",
		"update:latest": "bun --recursive update --interactive --latest",
		"typecheck": "bun run --recursive typecheck",
		"release": "bun ./packages/actions/src/releasePackages/index.ts"
	},
	"type": "module",
	"contributors": [
		"Crawl <icrawltogo@gmail.com>",
		"Amish Shah <amishshah.2k@gmail.com>",
		"Vlad Frangu <me@vladfrangu.dev>",
		"SpaceEEC <spaceeec@yahoo.com>",
		"Aura Rom√°n <kyradiscord@gmail.com>"
	],
	"keywords": [
		"discord",
		"api",
		"bot",
		"client",
		"node",
		"discordapp"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/discordjs/discord.js.git"
	},
	"bugs": {
		"url": "https://github.com/discordjs/discord.js/issues"
	},
	"homepage": "https://discord.js.org",
	"funding": "https://github.com/discordjs/discord.js?sponsor",
	"devDependencies": {
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-angular": "^20.2.0",
		"@favware/cliff-jumper": "^6.0.0",
		"@favware/npm-deprecate": "^2.0.0",
		"@sapphire/snowflake": "^3.5.5",
		"@types/bun": "^1.3.8",
		"@types/lodash.merge": "^4.6.9",
		"@unocss/eslint-plugin": "^66.5.10",
		"conventional-changelog-cli": "^5.0.0",
		"discord-api-types": "^0.38.38",
		"eslint": "^9.39.2",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"lodash.merge": "^4.6.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"unocss": "^66.5.10",
		"vercel": "^49.1.2"
	},
	"engines": {
		"bun": ">=1.0.0"
	},
	"packageManager": "bun@1.0.0",
	"dependencies": {
		"ts-mixer": "^6.0.4"
	}
}
