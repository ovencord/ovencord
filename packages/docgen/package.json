		"eslint-formatter-compact": "^9.0.1",
		"eslint-formatter-pretty": "^7.0.0",
		"prettier": "^3.7.4",
		"typescript": "^5.9.3",
		"@types/bun": "latest",
		"typescript-eslint": "^8.54.0"
