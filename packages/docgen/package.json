{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@ovencord/docgen",
	"version": "0.12.1",
	"description": "The docs.json generator for discord.js and its related projects",
	"scripts": {
		"test": "bun test",
		"typecheck": "tsc --noEmit",
